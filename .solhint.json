{
  "extends": "solhint:default",
  "plugins": [],
  "rules": {
      "const-name-snakecase": "off",
      "avoid-suicide": "error",
      "avoid-sha3": "warn",
      "not-rely-on-block-hash": "warn",
      "max-line-length": ["warn", 140],
      "no-unused-imports": "warn",
      "imports-order": "warn"
  }
}